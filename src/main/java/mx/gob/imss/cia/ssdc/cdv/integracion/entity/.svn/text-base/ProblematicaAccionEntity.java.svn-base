package mx.gob.imss.cia.ssdc.cdv.integracion.entity;

import java.io.Serializable;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

/**
 * @author propietario
 *
 */

@Entity
@Table(name = "CDT_PROBL_ACCION")
public class ProblematicaAccionEntity implements Serializable {
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	@Id
	private LlaveCompProblAccion llave;
	@Column(name = "NUM_ACCIONREALIZADA")
	private Integer accionRealizada;
	/**
	 * @return the llave
	 */
	public LlaveCompProblAccion getLlave() {
		return llave;
	}
	/**
	 * @param llave the llave to set
	 */
	public void setLlave(LlaveCompProblAccion llave) {
		this.llave = llave;
	}
	/**
	 * @return the accionRealizada
	 */
	public Integer getAccionRealizada() {
		return accionRealizada;
	}
	/**
	 * @param accionRealizada the accionRealizada to set
	 */
	public void setAccionRealizada(Integer accionRealizada) {
		this.accionRealizada = accionRealizada;
	}
	
	

}
