package mx.gob.imss.cia.ssdc.cdv.servicios;

import org.springframework.core.NestedCheckedException;

/**
 * 
 * @author
 * 
 */
public class SsdcException extends NestedCheckedException {
	
	private static final long serialVersionUID = 1L;
	
	public SsdcException(String msg) {
		super(msg);
	}

	public SsdcException(String mensaje, Throwable causa) {
		super(mensaje, causa);
	}

	
}
